#!/bin/sh
path=../ui/styles
cat $path/src/base.css > $path/src/styles.css
cat $path/src/bootstrap.min.css >> $path/src/styles.css
cat $path/src/screen.css >> $path/src/styles.css
yuicompressor --type css -o $path/styles-min.css $path/src/styles.css
yuicompressor --type css -o $path/styles-poor-min.css $path/src/styles-poor.css